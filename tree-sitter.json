{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lambdapi",
      "camelcase": "LambdaPi",
      "title": "tree-sitter-lambdapi",
      "scope": "source.lambdapi",
      "file-types": [
        "lambdapi"
      ],
      "injection-regex": "^lambdapi$",
      "class-name": "TreeSitterLambdapi"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "LambdaPi grammar for tree-sitter",
    "authors": [
      {
        "name": "Ciaran Dunne",
        "email": "cciaranddunne@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/ciaran-matthew-dunne/tree-sitter-lambdapi"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}